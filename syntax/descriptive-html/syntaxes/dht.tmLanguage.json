{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Dht",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.dht",
          "match": "\\b(template|make|with|as|and|end|import|use)\\b",
          "comment": "for you"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.dht",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.dht",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.dht"
}
